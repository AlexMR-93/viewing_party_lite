<h1>log in</h1>

<%= form_with url: login_path, method: :post, local: true do |form| %>
<%= form.hidden_field :name %>
<%= form.label :email, "Email: " %>
<%= form.text_field :email %>
<br><br>
<%= form.label :password, "Password: " %>
<%= form.password_field :password %>
<br><br>
<%= form.submit 'Log In' %>
<% end %>
