<%=button_to "Discover Movies", user_discover_path(@user), method: :get%>
<%@results.each do |result|%>
  <p>
  <%=link_to result.title, user_movie_path(@user, result.id) %>
  Vote Average: <%=result.vote_average%>
  <img src=<%=result.pic%> alt="images" width="100" height="100">
  <br>
  </p>
<%end%>
